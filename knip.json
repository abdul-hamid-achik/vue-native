{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    "packages/runtime": {
      "ignoreDependencies": ["@vue/shared"]
    },
    "packages/navigation": {},
    "packages/vite-plugin": {
      "ignoreDependencies": ["vite"]
    },
    "packages/cli": {},
    "examples/*": {}
  },
  "ignore": [
    "native/**",
    "docs/**",
    "tools/**",
    "**/*.d.ts"
  ],
  "exclude": ["classMembers", "exports", "types"],
  "ignoreBinaries": ["swift"]
}
