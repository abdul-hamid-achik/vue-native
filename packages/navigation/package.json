{
  "name": "@thelacanians/vue-native-navigation",
  "version": "0.2.0",
  "description": "Stack navigation for Vue Native apps",
  "license": "MIT",
  "author": "Vue Native Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdul-hamid-achik/vue-native",
    "directory": "packages/navigation"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.5.0",
    "@thelacanians/vue-native-runtime": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.9"
  }
}
