{
  "name": "@thelacanians/vue-native-cli",
  "version": "0.4.11",
  "description": "CLI for creating and running Vue Native apps",
  "license": "MIT",
  "author": "Vue Native Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdul-hamid-achik/vue-native",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": {
    "vue-native": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/config.js",
    "./cli": "./dist/cli.js"
  },
  "files": ["dist", "README.md", "native"],
  "scripts": {
    "prebuild": "rm -rf native && cp -r ../../native native",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "ws": "^8.18.0",
    "chokidar": "^3.6.0",
    "picocolors": "^1.1.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13",
    "@types/node": "^22.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
