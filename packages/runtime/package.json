{
  "name": "@thelacanians/vue-native-runtime",
  "version": "0.4.12",
  "description": "Vue 3 custom renderer for native iOS and Android apps",
  "license": "MIT",
  "author": "Vue Native Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdul-hamid-achik/vue-native",
    "directory": "packages/runtime"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.5.0",
    "@vue/reactivity": "^3.5.0",
    "@vue/shared": "^3.5.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.7.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
